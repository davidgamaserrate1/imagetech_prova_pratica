// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SimpleProduct{
  id Int @id @default(autoincrement())
  description String?
  value Int
}

model DigitalProduct{
  id Int @id @default(autoincrement())
  description String?
  value Int
  linkDownload String
}